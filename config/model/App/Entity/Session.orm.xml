<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Session" table="session">
    <id name="id" type="string" length="128"/>
    <field name="data" type="blob" nullable="true"/>
    <field name="time" type="integer" nullable="true"/>
    <field name="lifetime" type="integer" nullable="true"/>
  </entity>
</doctrine-mapping>
